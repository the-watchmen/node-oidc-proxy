{
  "name": "@watchmen/oidc-proxy",
  "version": "1.1.6",
  "description": "",
  "main": "dist/index.js",
  "author": "tk",
  "license": "MIT",
  "scripts": {
    "start": "cross-env DEBUG=dbg:*,app:* babel-watch src",
    "start-test": "cross-env NODE_ENV=auto DEBUG=app:* babel-watch src/api",
    "ava": "cross-env DEBUG=dbg:*,test:*,app:*,lib:*,oidc-provider:* NODE_ENV=auto ava -s test/ava",
    "ava-debug": "cross-env DEBUG=dbg:*,test:*,app:*,lib:*,oidc-provider:* NODE_ENV=auto ava -s",
    "pretest": "xo",
    "test": "yarn run ava && yarn run cuke",
    "prebuild": "yarn run test",
    "build": "babel ./src -d ./dist",
    "fix": "xo --fix",
    "test-proxy": "cross-env DEBUG=dbg:*,app:*,lib:*,test:* babel-watch test/shared/test-proxy",
    "mock-api": "cross-env DEBUG=dbg:*,app:*,lib:*,test:* babel-node mock-server",
    "mock-idp":
      "cross-env DEBUG=dbg:*,app:*,lib:*,test:*,oidc-provider:* babel-watch test/shared/mock-oidc-provider",
    "proxy-all": "yarn run mock-api & yarn run mock-idp & yarn run test-proxy",
    "cuke":
      "cross-env DEBUG=dbg:*,test:*,app:*,lib:*,oidc-provider:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register test/cuke",
    "cuke-feature":
      "cross-env DEBUG=dbg:*,test:*,app:*,lib:*,oidc-provider:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register"
  },
  "dependencies": {
    "@watchmen/debug": "1.1.1",
    "body-parser": "1.18.2",
    "config": "1.29.4",
    "cors": "2.8.4",
    "express": "4.16.2",
    "express-http-proxy": "1.1.0",
    "express-session": "1.15.6",
    "fs-extra": "5.0.0",
    "lodash": "4.17.5",
    "openid-client": "1.19.4"
  },
  "comments": {
    "1": "@3846masa/axios-cookiejar-support > 0.1.4 breaks stuff",
    "2": "cucumber > 3 breaks stuff"
  },
  "devDependencies": {
    "@3846masa/axios-cookiejar-support": "0.1.4",
    "@watchmen/cuke-helpr": "3.2.0",
    "@watchmen/helpr": "2.3.1",
    "@watchmen/json-server-helpr": "1.1.0",
    "@watchmen/test-helpr": "0.3.4",
    "@watchmen/web-helpr": "2.3.0",
    "ava": "0.25.0",
    "axios": "0.17.1",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-import": "1.6.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-watch": "2.0.7",
    "base64url": "2.0.0",
    "child-process-promise": "2.2.1",
    "cross-env": "5.1.3",
    "cucumber": "3.2.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "express-jwt": "5.3.0",
    "faker": "4.1.0",
    "htmlparser2": "3.9.2",
    "json-server": "0.12.1",
    "jsonwebtoken": "8.1.1",
    "node-jose": "0.11.0",
    "oidc-provider": "2.13.1",
    "prettier": "1.10.2",
    "randexp": "0.4.8",
    "tough-cookie": "2.3.3",
    "xo": "0.20.2"
  },
  "ava": {
    "require": ["babel-register", "babel-polyfill"]
  },
  "xo": {
    "useEslintrc": true
  },
  "publishConfig": {
    "access": "public"
  }
}
