{
  "name": "@watchmen/oidc-proxy",
  "version": "2.0.0",
  "description": "",
  "main": "dist/index.js",
  "author": "tk",
  "license": "MIT",
  "scripts": {
    "start": "cross-env DEBUG=dbg:*,app:* babel-watch src",
    "start-test": "cross-env NODE_ENV=auto DEBUG=app:* babel-watch src/api",
    "ava": "cross-env NODE_ENV=auto DEBUG=dbg:*,test:*,app:*,lib:*,oidc-provider:* ava -s",
    "pretest": "xo",
    "test": "echo 'WARNING: tests disabled...'",
    "test-": "NODE_ENV=auto yarn ava test/ava && yarn run cuke",
    "prebuild": "yarn run test",
    "build": "babel ./src -d ./dist",
    "fix": "xo --fix",
    "run-all": "cross-env NODE_ENV=auto DEBUG=dbg:*,app:*,lib:*,test:*,oidc-provider:*,oidc-client:* babel-node test/shared/run-all",
    "cuke": "cross-env DEBUG=dbg:*,test:*,app:*,lib:*,oidc-provider:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register test/cuke",
    "cuke-feature": "cross-env DEBUG=dbg:*,test:*,app:*,lib:*,oidc-provider:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@godaddy/terminus": "4.1.2",
    "@watchmen/debug": "1.1.1",
    "body-parser": "1.19.0",
    "config": "3.1.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-http-proxy": "1.5.1",
    "express-session": "1.16.1",
    "fs-extra": "8.0.1",
    "lodash": "4.17.11",
    "nocache": "^2.1.0",
    "openid-client": "3.1.1"
  },
  "devDependencies": {
    "@3846masa/axios-cookiejar-support": "0.4.2",
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.4.4",
    "@babel/register": "7.4.4",
    "@watchmen/cuke-helpr": "3.2.0",
    "@watchmen/helpr": "2.3.8",
    "@watchmen/json-server-helpr": "1.1.0",
    "@watchmen/test-helpr": "0.3.4",
    "@watchmen/web-helpr": "2.3.1",
    "ava": "1.4.1",
    "axios": "0.18.0",
    "base64url": "3.0.1",
    "child-process-promise": "2.2.1",
    "cross-env": "5.2.0",
    "cucumber": "5.1.0",
    "express-jwt": "5.3.1",
    "faker": "4.1.0",
    "htmlparser2": "3.10.1",
    "json-server": "0.15.0",
    "jsonwebtoken": "8.5.1",
    "node-jose": "1.1.3",
    "oidc-provider": "5.5.5",
    "pretty": "^2.0.0",
    "randexp": "0.5.3",
    "tough-cookie": "3.0.1",
    "xo": "0.24.0"
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "xo": {
    "prettier": true,
    "rules": {
      "eslint-comments/disable-enable-pair": 0,
      "import/no-unassigned-import": 0,
      "import/prefer-default-export": 0,
      "capitalized-comments": 0,
      "new-cap": 0,
      "radix": 0,
      "no-unused-expressions": [
        2,
        {
          "allowShortCircuit": true,
          "allowTernary": true
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
